man:

distclean:
		${MAKE} clean

autoclean:
		${MAKE} distclean

clean:

test:		

install:
		mkdir -p $(datarootdir)@mandir@/man8
		for f in *.8; do \
			install -m 644 $$f $(datarootdir)@mandir@/man8; \
		done
